\par }{\plain \i\f3\fs20\lang2057   file\}}
\par   {\plain \f3\fs20\lang2057 PJVersionInfo1.FileName := 'c:\\windows\\calc.exe';}
\par \pard\plain \intbl \f2\fs20\lang2057 {\f3   }{\b\f3 with}{\f3  Memo1.Lines }{\b\f3 do}{\f3 
